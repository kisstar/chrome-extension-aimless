{
  "files.associations": {
    "*.svg": "html"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": ["source.fixAll"],
  "editor.formatOnSave": true,
  "eslint.validate": ["javascript", "react"],
  "stylelint.validate": ["css"],
  "cSpell.words": [
    "bilibili",
    "csdn",
    "dlink",
    "partialize",
    "tampermonkey",
    "webext"
  ]
}
