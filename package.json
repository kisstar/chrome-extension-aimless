{
  "name": "chrome-extension-aimless",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A highly scalable and multifunctional browser extension.",
  "license": "MIT",
  "scripts": {
    "dev:core": "pnpm --filter chrome-extension-aimless dev",
    "dev:test": "pnpm --filter chrome-extension-aimless dev:test",
    "dev:tb": "pnpm --filter toolbox dev",
    "dev:ms": "node build/vite.build.mjs --watch",
    "dev:doc": "pnpm --filter @chrome-extension-aimless/docs dev",
    "build:core": "pnpm --filter chrome-extension-aimless build",
    "build:tb": "pnpm --filter toolbox build",
    "build:ms": "node build/vite.build.mjs",
    "build:doc": "pnpm --filter @chrome-extension-aimless/docs build",
    "lint": "eslint",
    "build": "npm run build:doc && npm run build:ms",
    "deploy": "npm run build && gh-pages -d .output/docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "eslint": "catalog:",
    "gh-pages": "catalog:",
    "vite": "catalog:"
  }
}
